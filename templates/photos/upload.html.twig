{% extends 'base.html.twig' %}
{% block body %}
    <div class="container">
        <div class="col-lg-12">
            <form action="/photos/upload" method="post" enctype="multipart/form-data">
                <div class="form-group">
                    <label for="test" class="col-form-label">Изображения</label>
                    <input type="file" name="file[]" multiple accept="images/*">
                </div>
                <button type="submit">Сохранить</button>
            </form>
        </div>
    </div>
{% endblock %}